from mit_core.vm_data import Instruction, InternalExtraInstruction

print('''\
// Auto-generated by gen-opcodes - DO NOT EDIT.
//
// Instruction opcodes.

#ifndef MIT_OPCODES
#define MIT_OPCODES


#define INSTRUCTION(name, opcode) name = opcode,
''')

Instruction.print("O_")
InternalExtraInstruction.print("O_EXTRA_")

print('''
#undef INSTRUCTION


#endif
''')
