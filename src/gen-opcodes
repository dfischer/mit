from smite_core.vm_data import Instruction

print('''\
// Auto-generated by gen-opcodes - DO NOT EDIT.
//
// Instruction opcodes.

#ifndef SMITE_OPCODES
#define SMITE_OPCODES


#define INSTRUCTION(name, opcode) name = opcode,
''')

Instruction.print("O_")

print('''
#undef INSTRUCTION


#endif
''')
