from mit_core.code_buffer import Code
from mit_core.vm_data import Register


code = Code('// Auto-generated by gen-registers - DO NOT EDIT.')
for key in sorted(Register.__members__.keys()):
    code.append("R({})".format(str(key)))
print(code.unindented_str())