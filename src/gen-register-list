#!/usr/bin/env python3

from vm_data import Register, registers

registers.sort(key=lambda r : r.name)

print('// Auto-generated by gen-register-list - DO NOT EDIT.\n')
for register in registers:
    print("R", end='')
    if register.read_only:
        print("_RO", end='')
    print("({}, {}, {})".format(register.name, register.ty, register.uty))
