#!/usr/bin/env python3

from vm_data import Registers

sorted_keys = list(Registers.__members__.keys())
sorted_keys.sort()

print('// Auto-generated by gen-register-list - DO NOT EDIT.\n')
for key in sorted_keys:
    register = Registers[key].value
    print("R", end='')
    if register.read_only:
        print("_RO", end='')
    print("({}, {}, {})".format(key, register.ty, register.uty))
