#!/usr/bin/env python3

from opcodes_extra import *

def print_enum(enum, prefix):
    print('\nenum {')
    for (name, opcode) in enum.__members__.items():
        print("    {}{} = {:#x},".format(prefix, name, opcode))
    print('};')


print('// Auto-generated by gen-opcodes-extra - DO NOT EDIT.\n')
print_enum(LibActions, "")
print_enum(LibcLib, "LIB_C_")
print_enum(SMiteLib, "LIB_SMITE_")
