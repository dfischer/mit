#!/usr/bin/env python3

from smite_core.vm_data_extra import *

def print_enum(actions, prefix):
    print('\nenum {')
    for (instruction, action) in actions.__members__.items():
        print("    INSTRUCTION({}{}, {:#x})".format(prefix, instruction, action.value.opcode))
    print('};')


print('// Auto-generated by gen-opcodes-extra - DO NOT EDIT.\n')
print_enum(LibActions, "")
print_enum(LibcLib, "LIB_C_")
print_enum(SMiteLib, "LIB_SMITE_")
