import mit_core.vm_data as vm_data
import mit_core.errors as errors
from mit_core.code_util import Code, enum_to_python

import extra_instructions


code = Code()
code.append('''\
# Auto-generated by gen-python-errors - DO NOT EDIT.
# Error enums.
#
# (c) Mit authors 2019
#
# The package is distributed under the MIT/X11 License.
#
# THIS PROGRAM IS PROVIDED AS IS, WITH NO WARRANTY. USE IS AT THE USER’S
# RISK.

from enum import IntEnum, unique
''')
code.append('')
code.extend(enum_to_python(vm_data.MitError))
code.append('')
code.extend(enum_to_python(errors.MallocError))
code.append('')
code.extend(enum_to_python(errors.LoadError))
code.append('')
code.extend(enum_to_python(errors.SaveError))

print(code)
