import mit_core.vm_data as vm_data
import mit_core.errors as errors
import extra_instructions

print('''\
# Auto-generated by gen-python-errors - DO NOT EDIT.
# Error enums.


from enum import IntEnum, unique
''')

print('''
@unique
class ExecutionError(IntEnum):''')
for error in vm_data.ExecutionError:
    print('    {} = {},'.format(error.name, error))

print('''
@unique
class MallocError(IntEnum):''')
for error in errors.MallocError:
    print('    {} = {},'.format(error.name, error))

print('''
@unique
class LoadError(IntEnum):''')
for error in errors.LoadError:
    print('    {} = {},'.format(error.name, error))

print('''
@unique
class SaveError(IntEnum):''')
for error in errors.SaveError:
    print('    {} = {},'.format(error.name, error))
